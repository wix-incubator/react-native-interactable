{
  "name": "react-native-interactable",
  "version": "0.1.10",
  "description": "High performance interactable views in React Native",
  "license": "MIT",
  "author": "Tal Kol <talkol@gmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/wix/react-native-interactable/issues"
  },
  "homepage": "https://github.com/wix/react-native-interactable",
  "readme": "https://github.com/wix/react-native-interactable#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/react-native-interactable.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "ln -sf lib/{ios,android} .",
    "example:android": "cd example && react-native run-android",
    "example:ios": "cd example && react-native run-ios",
    "playground:start":
      "node ./node_modules/react-native/local-cli/cli.js start --project-path ./playground/ios",
    "playground:android": "cd playground && react-native run-android",
    "playground:ios": "react-native run-ios --project-path ./playground/ios"
  },
  "dependencies": {
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.2"
  },
  "peerDependencies": {
    "react": ">= 15.4.1",
    "react-native": ">= 0.40.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "modulePaths": ["<rootDir>"]
  },
  "devDependencies": {
    "@types/jest": "22.2.0",
    "@types/react": "16.0.40",
    "@types/react-native": "0.52.18",
    "babel-jest": "23.0.0-alpha.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "react-native-typescript-transformer": "1.2.3",
    "react-test-renderer": "^16.3.0-alpha.1",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2"
  },
  "typings": "typings/react-native-interactable.d.ts"
}
